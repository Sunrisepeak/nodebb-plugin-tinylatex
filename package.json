{
  "name": "nodebb-plugin-tinylatex",
  "version": "0.0.1",
  "description": "A tiny latex plugins for nodebb",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sunrisepeak/nodebb-plugin-tinylatex"
  },
  "scripts": {
    "lint": "eslint ."
  },
  "keywords": [
    "nodebb",
    "plugin",
    "tinylatex",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sunrisepeak/nodebb-plugin-tinylatex"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-angular": "19.8.1",
    "eslint": "9.30.0",
    "eslint-config-nodebb": "1.1.8",
    "husky": "9.1.7",
    "lint-staged": "16.1.2"
  }
}
